namespace src\Batch\Entity;

use \core\Entity as Entity;

/**
 * Entity to group
 *
 * @category  	src
 * @package   	src\Batch\Entity
 * @author    	Judicaël Paquet <paquet.judicael@iscreenway.com>
 * @copyright 	Copyright (c) 2013-2014 iScreenway FR/VN Inc. (http://www.iscreenway.com)
 * @license   	http://www.iscreenway.com/framework/licence.php Tout droit réservé à http://www.iscreenway.com
 * @version   	Release: 1.0.0
 * @filesource	http://www.iscreenway.com/framework/download.php
 * @link      	http://www.iscreenway.com
 * @since     	1.0
 */

class group extends Entity {

	/**
	 * id
	 *
	 * @access private
	 * @var    int
	 *
	 * @primary_key
	 * @map id
	 */

	private $id = null;

	/**
	 * name
	 *
	 * @access private
	 * @var    varchar
	 *
	 */

	private $name = null;

	/**
	 * group_id
	 *
	 * @access private
	 * @var    int
	 *
	 */

	private $group_id = null;

	/**
	 * get id of group
	 *
	 * @access public
	 * @return int
	 */

	public function get_id() {

		return $this->id;
	}

	/**
	 * set id of group
	 *
	 * @access public
	 * @param  int $id id of group
	 * @return \src\Batch\Entity\group
	 */

	public function set_id($id) {

		$this->id = $id;
		return $this;
	}

	/**
	 * get name of group
	 *
	 * @access public
	 * @return varchar
	 */

	public function get_name() {

		return $this->name;
	}

	/**
	 * set name of group
	 *
	 * @access public
	 * @param  varchar $name name of group
	 * @return \src\Batch\Entity\group
	 */

	public function set_name($name) {

		$this->name = $name;
		return $this;
	}

	/**
	 * get group_id of group
	 *
	 * @access public
	 * @return int
	 */

	public function get_group_id() {

		return $this->group_id;
	}

	/**
	 * set group_id of group
	 *
	 * @access public
	 * @param  int $group_id group_id of group
	 * @return \src\Batch\Entity\group
	 */

	public function set_group_id($group_id) {

		$this->group_id = $group_id;
		return $this;
	}
}