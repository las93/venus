namespace src\Batch\Entity;

use \core\Entity as Entity;

/**
 * Entity to news
 *
 * @category  	src
 * @package   	src\Batch\Entity
 * @author    	Judicaël Paquet <paquet.judicael@iscreenway.com>
 * @copyright 	Copyright (c) 2013-2014 iScreenway FR/VN Inc. (http://www.iscreenway.com)
 * @license   	http://www.iscreenway.com/framework/licence.php Tout droit réservé à http://www.iscreenway.com
 * @version   	Release: 1.0.0
 * @filesource	http://www.iscreenway.com/framework/download.php
 * @link      	http://www.iscreenway.com
 * @since     	1.0
 */

class news extends Entity {

	/**
	 * id
	 *
	 * @access private
	 * @var    int
	 *
	 * @primary_key
	 * @map id
	 */

	private $id = null;

	/**
	 * title
	 *
	 * @access private
	 * @var    varchar
	 *
	 */

	private $title = null;

	/**
	 * get id of news
	 *
	 * @access public
	 * @return int
	 */

	public function get_id() {

		return $this->id;
	}

	/**
	 * set id of news
	 *
	 * @access public
	 * @param  int $id id of news
	 * @return \src\Batch\Entity\news
	 */

	public function set_id($id) {

		$this->id = $id;
		return $this;
	}

	/**
	 * get title of news
	 *
	 * @access public
	 * @return varchar
	 */

	public function get_title() {

		return $this->title;
	}

	/**
	 * set title of news
	 *
	 * @access public
	 * @param  varchar $title title of news
	 * @return \src\Batch\Entity\news
	 */

	public function set_title($title) {

		$this->title = $title;
		return $this;
	}
}