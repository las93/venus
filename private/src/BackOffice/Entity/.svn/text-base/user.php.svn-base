namespace src\BackOffice\Entity;

use \core\Entity as Entity;

/**
 * Entity to user
 *
 * @category  	src
 * @package   	src\BackOffice\Entity
 * @author    	vietphan
 * @copyright 	Copyright (c) 2013-2014 iScreenway FR/VN Inc. (http://www.iscreenway.com)
 * @license   	http://www.iscreenway.com/framework/licence.php Tout droit réservé à http://www.iscreenway.com
 * @version   	Release: 1.0.0
 * @filesource	http://www.iscreenway.com/framework/download.php
 * @link      	http://www.iscreenway.com
 * @since     	1.0
 */

class user extends Entity {

	/**
	 * id
	 *
	 * @access private
	 * @var    int
	 *
	 * @primary_key
	 * @map id
	 */

	private $id = null;

	/**
	 * group_id
	 *
	 * @access private
	 * @var    int
	 *
	 */

	private $group_id = null;

	/**
	 * login
	 *
	 * @access private
	 * @var    varchar
	 *
	 */

	private $login = null;

	/**
	 * password
	 *
	 * @access private
	 * @var    varchar
	 *
	 */

	private $password = null;

	/**
	 * firstname
	 *
	 * @access private
	 * @var    varchar
	 *
	 */

	private $firstname = null;

	/**
	 * lastname
	 *
	 * @access private
	 * @var    varchar
	 *
	 */

	private $lastname = null;

	/**
	 * get id of user
	 *
	 * @access public
	 * @return int
	 */

	public function get_id() {

		return $this->id;
	}

	/**
	 * set id of user
	 *
	 * @access public
	 * @param  int $id id of user
	 * @return \src\BackOffice\Entity\user
	 */

	public function set_id($id) {

		$this->id = $id;
		return $this;
	}

	/**
	 * get group_id of user
	 *
	 * @access public
	 * @return int
	 */

	public function get_group_id() {

		return $this->group_id;
	}

	/**
	 * set group_id of user
	 *
	 * @access public
	 * @param  int $group_id group_id of user
	 * @return \src\BackOffice\Entity\user
	 */

	public function set_group_id($group_id) {

		$this->group_id = $group_id;
		return $this;
	}

	/**
	 * get login of user
	 *
	 * @access public
	 * @return varchar
	 */

	public function get_login() {

		return $this->login;
	}

	/**
	 * set login of user
	 *
	 * @access public
	 * @param  varchar $login login of user
	 * @return \src\BackOffice\Entity\user
	 */

	public function set_login($login) {

		$this->login = $login;
		return $this;
	}

	/**
	 * get password of user
	 *
	 * @access public
	 * @return varchar
	 */

	public function get_password() {

		return $this->password;
	}

	/**
	 * set password of user
	 *
	 * @access public
	 * @param  varchar $password password of user
	 * @return \src\BackOffice\Entity\user
	 */

	public function set_password($password) {

		$this->password = $password;
		return $this;
	}

	/**
	 * get firstname of user
	 *
	 * @access public
	 * @return varchar
	 */

	public function get_firstname() {

		return $this->firstname;
	}

	/**
	 * set firstname of user
	 *
	 * @access public
	 * @param  varchar $firstname firstname of user
	 * @return \src\BackOffice\Entity\user
	 */

	public function set_firstname($firstname) {

		$this->firstname = $firstname;
		return $this;
	}

	/**
	 * get lastname of user
	 *
	 * @access public
	 * @return varchar
	 */

	public function get_lastname() {

		return $this->lastname;
	}

	/**
	 * set lastname of user
	 *
	 * @access public
	 * @param  varchar $lastname lastname of user
	 * @return \src\BackOffice\Entity\user
	 */

	public function set_lastname($lastname) {

		$this->lastname = $lastname;
		return $this;
	}
}